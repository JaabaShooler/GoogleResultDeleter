{
  "manifest_version": 3,
  "name": "Google Result Deleter",
  "version": "1.0",
  "description": "A browser extension to delete the first Google search result.",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Google Result Deleter"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "host_permissions": [
    "http://google.com/search?q=*",
    "https://google.com/search?q*"
  ]
}
